		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.3.2"
	},
	"dependencies": {
		"@lucide/svelte": "^0.503.0",
		"@portabletext/svelte": "^3.0.0",
		"@sanity/client": "^7.2.1",
